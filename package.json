{
  "name": "twilight-cosmos-theme",
  "displayName": "Twilight Cosmos",
  "description": "A dark theme inspired by Twilight with cosmic colors",
  "version": "1.0.12",
  "publisher": "zellwk",
  "icon": "assets/icon2.png",
  "type": "module",
  "exports": {
    ".": "./themes/twilight-cosmos-custom.json",
    "./js": "./src/twilight-cosmos-theme.js"
  },
  "scripts": {
    "package": "npx vsce package",
    "build": "node build-theme.js",
    "publish:vscode": "npx dotenv-cli -e .env -- npx vsce publish",
    "publish:openvsx": "npx dotenv-cli -e .env -- npx ovsx publish",
    "publish:npm": "npm publish"
  },
  "files": [
    "src",
    "themes",
    "README.md",
    "LICENSE",
    "assets"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "twilight",
    "cosmos",
    "color-theme",
    "dark",
    "purple",
    "blue",
    "vscode-theme"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Twilight Cosmos",
        "uiTheme": "vs-dark",
        "path": "./themes/twilight-cosmos-custom.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zellwk/twilight-cosmos"
  },
  "license": "MIT"
}
